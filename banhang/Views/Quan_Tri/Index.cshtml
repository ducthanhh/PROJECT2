@model IEnumerable<banhang.Models.Quan_Tri>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table table-bordered">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Mat_Khau)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Trang_Thai)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Mat_Khau)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Trang_Thai)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Tai_Khoan }) |
            @Html.ActionLink("Details", "Details", new { id=item.Tai_Khoan }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Tai_Khoan })
        </td>
    </tr>
}

</table>
